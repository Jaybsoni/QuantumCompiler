    "from qiskit import * \n",
    "import numpy as np\n",
    "\n",
    "circ = qiskit.QuantumCircuit(3)\n",
    "circ.i(0)\n",
    "circ.h(0)\n",
    "circ.x(0)\n",
    "circ.y(0)\n",
    "circ.z(0)\n",
    "circ.rx(np.pi/2, 0)\n",
    "circ.ry(np.pi/2, 0)\n",
    "circ.rz(np.pi/2, 0)\n",
    "circ.cx(0, 2)\n",
    "circ.cz(0, 2)\n",
    "\n",
    "circ.draw()"
